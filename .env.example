# Netlify DB (Neon Postgres): auto-provisioned by Netlify, set manually for local dev
NETLIFY_DATABASE_URL=postgres://user:password@host/dbname

# Resend email service: get from resend.com dashboard (needed for Phase 7+)
RESEND_API_KEY=re_xxxxxxxxxxxx

# Public city configuration: set in Netlify env vars dashboard
PUBLIC_CITY_NAME=Pittsburgh
PUBLIC_CITY_DOMAIN=pittsburgh.comedyconnector.app
PUBLIC_SITE_URL=https://pittsburgh.comedyconnector.app
